  "scripts": {
    "start:fe": "react-scripts start",
    "build:fe": "react-scripts build",
    "build:backend": "tsc src/backend/**/** --outDir backend/",
    "start:be": "node backend/server.js",
    "build": "npm run build:fe && npm run build:backend",
    "dev:server": "nodemon --watch 'src/backend/**/*.ts' --exec 'ts-node' src/backend/server.ts",
    "test": "react-scripts src/test -- --config=jest.config.js",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@types/lodash-es": "^4.17.6",
